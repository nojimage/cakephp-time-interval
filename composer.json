{
    "name": "elstc/cakephp-time-interval",
    "license": "MIT",
    "description": "TimeInterval type plugin for CakePHP",
    "type": "cakephp-plugin",
    "require": {
        "php": ">=5.6",
        "ext-pdo": "*",
        "ext-json": "*",
        "cakephp/cakephp": "^3.4"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7|^6.0",
        "cakephp/cakephp-codesniffer": "^3.1"
    },
    "autoload": {
        "psr-4": {
            "Elastic\\TimeInterval\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Elastic\\TimeInterval\\Test\\": "tests",
            "Cake\\Test\\": "./vendor/cakephp/cakephp/tests"
        }
    }
}
